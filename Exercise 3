def ctrapezoidal(f, a, b, n):
    def s(f,a,b,n):
        x=0
        for i in range(1,n-1):
            x=f(a+(i/n)*(b-a)) + x
        return x
    h=(b-a)/n
    return (h/2)*(f(a)+f(b)) + h*s(f,a,b,n)

p = lambda x: 6*x**2

Ih=[]
b=1
a=0
n=100
I=1

for n in range (1,n):
    a=abs(ctrapezoidal(p, 0, 1, n)-I)
    Ih.append(a)

hlist=[]
for n in range (1,n):
    hlist.append((b-a)/n)
